{
    "$schema": "./worker/node_modules/wrangler/config-schema.json",
    "name": "pdf-to-text-extraction-service",
    "main": "worker/src/index.ts",
    "compatibility_date": "2026-01-25",
    "r2_buckets": [
        {
            "binding": "PDF_BUCKET",
            "bucket_name": "users-knowledge-base"
        }
    ],
    "containers": [
        {
            "class_name": "PdfProcContainer",
            "image": "./Dockerfile",
            "max_instances": 10
        }
    ]
}